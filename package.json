{
  "name": "reactn-ant-jest",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.23.4",
    "axios": "^0.19.0",
    "babel-plugin-import": "^1.12.2",
    "customize-cra": "^0.7.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "moment": "^2.29.2",
    "moment-timezone": "^0.5.26",
    "ramda": "^0.26.1",
    "react": "^16.9.0",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.1.0",
    "react-scripts": "3.1.2",
    "reactn": "^2.2.4",
    "styled-components": "^4.4.0"
  },
  "scripts": {
    "start": "REACT_APP_VERSION=$npm_package_version REACT_APP_DEV_MODE=true react-app-rewired start",
    "build": "REACT_APP_VERSION=$npm_package_version PUBLIC_URL=https://hervispichardo.com/ react-app-rewired build",
    "deploy": "sh deploy.sh",
    "test": "jest --verbose ./tests",
    "test:watch": "jest --watch --verbose false ./tests",
    "eject": "react-app-rewired eject"
  },
  "homepage": "https://hervispichardo.com/reactn-ant-jest/",
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
